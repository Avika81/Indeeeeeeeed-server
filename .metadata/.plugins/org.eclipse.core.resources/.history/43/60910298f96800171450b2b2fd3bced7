import Main.Stack;

public class CountStack {		
		private int items = 0;
		Stack stack;
		
		public Object pop() {
			if(items>0)
				items--;
			return stack.pop();
		}
		
		public void push(int item) {
			items++;
			stack.push(item);
		}
		
		public int getItems() {
			return items;
		}
	}
	
	class Stack {
		Node top;
		
		Object pop() {
			if (top != null) {
				int item = top.data;
				top = top.next;
				return item;
			}
			return null;
		}
		
		void push(int item) {
			Node t = new Node(item);
			t.next = top;
			t.min = t.data < top.min.data ? t : top.min;			
			top = t;
		}
	}
